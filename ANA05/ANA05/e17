import numpy as np

# Crear la primera matriz, por ejemplo, una matriz de 2x3
matriz1 = np.array([[1, 2, 3],
                   [4, 5, 6]])

# Crear la segunda matriz, por ejemplo, una matriz de 3x2
matriz2 = np.array([[7, 8],
                   [9, 10],
                   [11, 12]])

# Realizar la multiplicación entre las dos matrices
resultado = np.dot(matriz1, matriz2)

# Mostrar el resultado en pantalla
print("Resultado de la multiplicación:")
print(resultado)
